{
    "manifest_version": 2,
    "name": "Youtube Music Video Or Song Image",
    "version": "1.1",
    "description": "Choose between video or song image on youtube music",
    "icons": {
        "48": "icons/icon-48.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://music.youtube.com/*"
            ],
            "js": [
                "main.js"
            ]
        }
    ]
}